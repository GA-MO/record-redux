{
  "name": "record-redux",
  "version": "1.0.11",
  "description": "Record data in reducer",
  "main": "lib/index.js",
  "repository": {
    "url": "https://github.com/GA-MO/record-redux.git",
    "type": "git"
  },
  "author": "Thawatchai Kuansombat (thawatchai.deartk@gmail.com)",
  "license": "MIT",
  "keywords": [
    "record",
    "replay",
    "redux"
  ],
  "homepage": "https://github.com/GA-MO/record-redux",
  "bugs": "https://github.com/GA-MO/record-redux/issues",
  "files": [
    "lib/index.js",
    "lib/actions.js",
    "lib/helpers.js",
    "lib/recordRedux.js",
    "lib/recordReduxItem.js",
    "lib/recordReduxMiddleware.js",
    "lib/style.css"
  ],
  "scripts": {
    "dev": "better-npm-run dev",
    "build": "npm run clean && better-npm-run build",
    "build:lib": "cross-env BABEL_ENV=production babel app/containers/recordRedux --out-dir lib",
    "build:all": "npm run build && npm run build:lib",
    "deploy": "npm version patch && npm publish",
    "clean": "better-npm-run clean",
    "test": "better-npm-run test"
  },
  "betterScripts": {
    "dev": {
      "command": "node devServer.js  --config webpack.config.dev.js",
      "env": {
        "NODE_ENV": "development"
      }
    },
    "build": {
      "command": "webpack --config webpack.config.production.js",
      "env": {
        "NODE_ENV": "production"
      }
    },
    "test": {
      "command": "mocha ./test/*.js --compilers js:babel-register --watch"
    },
    "clean": {
      "command": "rimraf lib"
    }
  },
  "devDependencies": {
    "babel-cli": "^6.5.1",
    "babel-core": "^6.3.15",
    "babel-eslint": "^6.1.2",
    "babel-loader": "^6.2.4",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-preset-es2015": "^6.3.13",
    "babel-preset-react": "^6.3.13",
    "babel-preset-react-hmre": "^1.0.1",
    "babel-preset-stage-0": "^6.5.0",
    "babel-register": "^6.3.13",
    "better-npm-run": "^0.0.13",
    "compression-webpack-plugin": "^0.3.1",
    "cross-env": "^1.0.7",
    "css-loader": "^0.23.1",
    "eslint": "^3.7.1",
    "eslint-config-airbnb": "^12.0.0",
    "eslint-import-resolver-webpack": "^0.5.1",
    "eslint-plugin-babel": "^4.0.0",
    "eslint-plugin-import": "^1.16.0",
    "eslint-plugin-jsx-a11y": "^2.2.2",
    "eslint-plugin-react": "^6.3.0",
    "eventsource-polyfill": "^0.9.6",
    "expect": "^1.20.1",
    "express": "^4.14.0",
    "extract-text-webpack-plugin": "^1.0.1",
    "file-loader": "^0.8.5",
    "mocha": "^2.5.3",
    "node-libs-browser": "^0.5.2",
    "node-sass": "^3.4.2",
    "react-addons-test-utils": "^0.14.7",
    "react-hot-loader": "^3.0.0-beta.1",
    "redux-logger": "^2.7.4",
    "es6-promise": "^3.2.1",
    "react": "^15.4.1",
    "react-dom": "^15.4.1",
    "react-redux": "^4.2.1",
    "redux": "^3.2.1",
    "redux-thunk": "^2.1.0",
    "rimraf": "^2.5.2",
    "sass-loader": "^3.2.0",
    "style-loader": "^0.13.1",
    "url-loader": "^0.5.7",
    "webpack": "^1.13.1",
    "webpack-dev-middleware": "^1.2.0",
    "webpack-dev-server": "^1.14.0",
    "webpack-hot-middleware": "^2.2.0"
  }
}
